#map_container_tools
{
    height: 36px;
    border-bottom: 1px solid #CAD2DD;
    box-shadow: 1px 2px 3px 0 #828E9A;
    background: #fff;
    position: relative;
    //z-index: 201;
}

.cityContainer
{
    position: absolute;
    float: right;
    margin-top: -6px;
    margin-left: 10px;
    z-index: 200;
    width: 420px;
    height: 396px;
    overflow: hidden;
    background: #FFF;
    color: #010000;
    font-size: 12px;
    box-shadow: 1px 2px 3px 0 #828E9A;
    border: 1px solid #dfddde;
    display: none;
}

.city-list-close-btn {
    height: 10px;
    width: 10px;
    float: right;
    margin: 5px 5px 0 0;
    background: url(http://www.amap.com/img/way_btn.png) -62px -12px no-repeat;
    cursor: pointer;
    line-height: 20px;
}

.city-list-title {
    padding: 5px 10px;
    height: 20px;
    line-height: 20px;
    border-bottom: 1px #dfddde solid;
}

.city-list-srh {
    padding-bottom: 20px;
    border-bottom: 1px #dfddde solid;
}
.city-list-item {
    padding: 10px;
    white-space: nowrap;
    overflow: hidden;
    zoom: 1;
}

.city-list-srh .city-srh-ipt {
    float: left;
    width: 220px;
    height: 22px;
    line-height: 22px;
    padding: 3px 5px;
    border: 1px solid #c5c5c5;
    border-right: 0;
}
.city-list .city-list-srh .city-list-ipt {
    outline: 0;
}

.city-list-srh .city-srh-btn {
    width: 80px;
    height: 30px;
    text-align: center;
    cursor: pointer;
    border: 1px solid #c6c6c6;
    line-height: 26px;
    float: left;
    background: -webkit-gradient(linear,0 0,0 100%,from(#FFF),to(#f1f1f1));
    background: -moz-linear-gradient(top,#FFF,#f1f1f1);
    background: linear-gradient(to bottom,#fff,#f1f1f1);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#FFFFFF,endColorstr=#F1F1F1,grandientType=0);
}

.city-list-content .city-scroll {
    height: 190px;
    margin-top: 10px;
}

.city-name {
    float: left;
    display: inline;
    line-height: 20px;
    height: 20px;
    margin-right: 10px;
    color: #2472df;
    text-decoration: none;
}

.city-scroll {
    height: 190px;
    margin-top: 10px;
}

.jspContainer {
    overflow: hidden;
    position: relative;
    overflow-y: auto;
    overflow-x: hidden;
}

.jspPane {
    position: absolute;
}

.city-content {
    width: 380px;
    overflow: hidden;
    zoom: 1;
}

.city-content-item {
    display: block;
    width: 380px;
    margin-bottom: 10px;
    overflow: hidden;
    zoom: 1;
}

.ui-corner-all {
    font-size: 14px;
    color: #000;
}
.ui-menu {
    list-style: none;
    padding: 2px;
    margin: 0;
    display: block;
    outline: 0;
    background: #fff;
    border: 1px solid #CCC;
}
.ui-autocomplete {
    position: absolute;
    top: 0;
    cursor: default;
    max-height: 254px;
    overflow-y: auto;
    z-index: 1040;
    float:none;
}

.ui-helper-hidden-accessible {
    position: absolute!important;
    clip: rect(1px 1px 1px 1px);
    clip: rect(1px,1px,1px,1px);
}

#map_left_ul li{
    float: left;
    margin-right: 20px;
}

input[type="radio"], input[type="checkbox"] {
    margin: 0; 
    margin-top: 1px;
    line-height: normal;
    cursor: pointer;
}